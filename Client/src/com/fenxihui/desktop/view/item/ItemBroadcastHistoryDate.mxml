<?xml version="1.0" encoding="utf-8"?>
<s:MXTreeItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009"
				xmlns:s="library://ns.adobe.com/flex/spark"
				xmlns:mx="library://ns.adobe.com/flex/mx"
				xmlns:component="com.fenxihui.library.component.*"
				height="30" autoDrawBackground="true">
	<s:layout>
		<s:HorizontalLayout/>
	</s:layout>
	
	<s:Label width="100%" height="100%" fontFamily="Georgia" fontSize="18"
			 fontWeight="bold" text="{data.@dateday}" verticalAlign="middle"/>
	<s:Label height="100%" color="#B60003" fontFamily="Georgia" fontSize="18" fontWeight="bold"
			 paddingLeft="5" paddingRight="5" text="{data.@count}" verticalAlign="middle"/>
	
</s:MXTreeItemRenderer>
